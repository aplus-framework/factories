{
    "name": "aplus/factories",
    "description": "Aplus Framework Factories Library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "factories",
        "factory",
        "template"
    ],
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "homepage": "https://aplus-framework.com/packages/factories",
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://github.com/aplus-framework/factories/issues",
        "forum": "https://aplus-framework.com/forum",
        "source": "https://github.com/aplus-framework/factories",
        "docs": "https://docs.aplus-framework.com/guides/libraries/factories/"
    },
    "funding": [
        {
            "type": "Aplus Sponsor",
            "url": "https://aplus-framework.com/sponsor"
        }
    ],
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^2.8",
        "ergebnis/composer-normalize": "^2.43",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^10.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Framework\\Factories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Factories\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
